<!DOCTYPE flight_plan SYSTEM "flight_plan.dtd">

<flight_plan alt="140" ground_alt="100" lat0="42.494966" lon0="-71.680976" max_dist_from_home="900" name="Basic" security_height="25">
  <header>
<!-- camp roberts 35.716429	-120.764792 -->
#include "subsystems/navigation/nav_line.h"
#include "datalink.h"
</header>
  <waypoints>
    <waypoint name="HOME" x="0" y="0"/>
    <waypoint name="STDBY" x="112.2" y="-10.2"/>
    <waypoint name="A" x="-46.3" y="-15.8"/>
    <waypoint name="B" x="122.5" y="-71.4"/>
    <waypoint name="MOB" x="165.4" y="193.3"/>
    <waypoint name="C" x="177.1" y="-9.6"/>
    <waypoint name="D" x="103.6" y="48.3"/>
    <waypoint alt="110.0" name="AF" x="191.3" y="-136.1"/>
    <waypoint alt="100" name="TD" x="163.3" y="-120.7"/>
    <waypoint name="_BASELEG" x="222.9" y="-155.2"/>
    <waypoint name="CLIMB" x="178.3" y="-127.8"/>
  </waypoints>
  <exceptions/>
  <blocks>
    <block name="Wait GPS">
      <set value="1" var="kill_throttle"/>
      <while cond="!GpsFixValid()"/>
    </block>
    <block name="Geo init">
      <while cond="LessThan(NavBlockTime(), 10)"/>
      <call fun="NavSetGroundReferenceHere()"/>
    </block>
    <block name="Holding point">
      <set value="0" var="kill_throttle"/>
      <attitude roll="0" throttle="0" vmode="throttle"/>
    </block>
    <block group="home" key="t" name="Takeoff" strip_button="Takeoff" strip_icon="takeoff.png">
      <exception cond="estimator_z > ground_alt+20" deroute="Standby"/>
      <set value="0" var="kill_throttle"/>
      <set value="0" var="estimator_flight_time"/>
<!--      <go from="HOME" pitch="15" throttle="1.0" vmode="throttle" wp="CLIMB"/> -->
      <attitude roll="0.0" pitch="45.0" throttle="0.40" vmode="throttle"/>
    </block>
    <block group="home" key="<Control>a" name="Standby" strip_button="Standby" strip_icon="home.png">
      <circle radius="nav_radius" wp="STDBY"/>
    </block>
    <block name="Skytruck - Roll">
      <attitude roll="nav_roll" climb="0.0" vmode="alt" alt="nav_alt" until="FALSE"/>
      <deroute block="Standby"/>
    </block>
    <block name="Skytruck - Heading" group="extra_pattern">
      <heading course="nav_course" alt="nav_alt" vmode="alt" until="FALSE"/>
      <deroute block="Standby"/>
    </block>
    <block group="base_pattern" key="F8" name="Figure 8 WP A,B" strip_button="Figure 8 (wp A,B)" strip_icon="eight.png">
      <eight center="A" radius="nav_radius" turn_around="B"/>
    </block>
    <block group="base_pattern" name="Oval A-B" strip_button="Oval (wp A-B)" strip_icon="oval.png">
      <oval p1="A" p2="B" radius="nav_radius"/>
    </block>
    <block group="base_pattern" name="MOB" strip_button="Turn around here" strip_icon="mob.png">
      <call fun="NavSetWaypointHere(WP_MOB)"/>
      <set value="DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <circle radius="nav_radius" wp="MOB"/>
    </block>
    <block group="extra_pattern" name="Line A-B" strip_button="Line (wp A-B)" strip_icon="line.png">
      <call fun="nav_line_init()"/>
      <call fun="nav_line(WP_A, WP_B, nav_radius)"/>
    </block>
    <block group="extra_pattern" name="Survey A-D" strip_button="Survey (wp A-D)" strip_icon="survey.png">
      <survey_rectangle grid="150" wp1="A" wp2="D"/>
    </block>
    <block group="extra_pattern" name="Path A,B,C,D" strip_button="Path (A,B,C,D)" strip_icon="path.png">
      <path wpts="A,B C"/>
      <path approaching_time="1" pitch="auto" throttle="0.4" wpts="C, D A"/>
      <deroute block="Path A,B,C,D"/>
    </block>
    <block group="extra_pattern" name="Path A,D,C,B" strip_icon="path.png">
      <path wpts="A,D C"/>
      <path approaching_time="1" pitch="auto" throttle="0.4" wpts="C, B A"/>
      <deroute block="Path A,D,C,B"/>
    </block>
    <block NAME="Follow Skate-S">
      <follow AC_ID="38" DISTANCE="10" HEIGHT="25"/>
    </block>
    <block group="land" name="Land Right AF-TD" strip_button="Land right (wp AF-TD)" strip_icon="land-right.png">
      <set value="DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block group="home" name="Glide Land">
<!--      <exception cond="ground_alt+5 > estimator_z" deroute="DZ Kill"/> -->
      <attitude roll="0.0" pitch="10" throttle="0.10" vmode="throttle"/>
    </block>
    <block group="land" name="Land Left AF-TD" strip_button="Land left (wp AF-TD)" strip_icon="land-left.png">
      <set value="-DEFAULT_CIRCLE_RADIUS" var="nav_radius"/>
      <deroute block="land"/>
    </block>
    <block name="land">
      <call fun="nav_compute_baseleg(WP_AF, WP_TD, WP__BASELEG, nav_radius)"/>
      <circle radius="nav_radius" until="NavCircleCount() > 0.5" wp="_BASELEG"/>
      <circle radius="nav_radius" until="And(NavQdrCloseTo(DegOfRad(baseleg_out_qdr)-10), 10 > fabs(estimator_z - WaypointAlt(WP__BASELEG)))" wp="_BASELEG"/>
    </block>
    <block name="final">
      <exception cond="ground_alt + 10 > estimator_z" deroute="flare"/>
      <go from="AF" hmode="route" vmode="glide" wp="TD"/>
    </block>
    <block group="home" name="Kill">
      <attitude roll="0.0" pitch="10" throttle="0.0" vmode="throttle"/>
      <set value="1" var="kill_throttle"/>
    </block>
    <block name="flare">
      <go approaching_time="0" from="AF" hmode="route" throttle="0.0" vmode="throttle" wp="TD"/>
      <attitude roll="0.0" throttle="0.0" until="FALSE" vmode="throttle"/>
    </block>
  </blocks>
</flight_plan>
